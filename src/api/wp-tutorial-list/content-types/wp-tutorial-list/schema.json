{
  "kind": "collectionType",
  "collectionName": "wp_tutorial_lists",
  "info": {
    "singularName": "wp-tutorial-list",
    "pluralName": "wp-tutorial-lists",
    "displayName": "WP Tutorial List",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "component",
      "repeatable": false,
      "component": "wp.default-label",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "tutorial "
      ],
      "required": true,
      "default": "tutorial "
    },
    "description": {
      "type": "component",
      "repeatable": false,
      "component": "wp.default-label"
    },
    "subcategories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::wp-subcategories-tutorial.wp-subcategories-tutorial",
      "mappedBy": "wp_tutorial_list"
    },
    "position": {
      "type": "biginteger",
      "required": true
    }
  }
}
