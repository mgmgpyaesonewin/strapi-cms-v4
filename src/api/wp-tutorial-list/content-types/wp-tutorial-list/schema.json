{
  "kind": "collectionType",
  "collectionName": "wp_tutorial_lists",
  "info": {
    "singularName": "wp-tutorial-list",
    "pluralName": "wp-tutorial-lists",
    "displayName": "WP Tutorial List",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "component",
      "repeatable": false,
      "component": "wp.default-label",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "story",
        "tutorial"
      ],
      "required": true
    },
    "wp_tutorial": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::wp-tutorial.wp-tutorial"
    },
    "wp_tutorial_story": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::wp-tutorial-story.wp-tutorial-story"
    }
  }
}
