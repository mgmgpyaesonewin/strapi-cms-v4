{
  "kind": "collectionType",
  "collectionName": "wp_tutorial_stories",
  "info": {
    "singularName": "wp-tutorial-story",
    "pluralName": "wp-tutorial-stories",
    "displayName": "WP Story Content",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "title": {
      "type": "component",
      "repeatable": false,
      "component": "wp.default-label"
    },
    "description": {
      "type": "component",
      "repeatable": false,
      "component": "wp.default-label"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "stories": {
      "displayName": "stories",
      "type": "component",
      "repeatable": true,
      "component": "wp.stories"
    },
    "options": {
      "displayName": "button option",
      "type": "component",
      "repeatable": false,
      "component": "wp.button-option"
    },
    "wp_tutorial_story_list": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::wp-tutorial-story-list.wp-tutorial-story-list",
      "inversedBy": "wp_tutorial_stories"
    },
    "position": {
      "type": "biginteger",
      "required": true,
      "min": "1"
    },
    "isPinned": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
