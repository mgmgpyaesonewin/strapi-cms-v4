{
  "kind": "collectionType",
  "collectionName": "merchant_mini_app_categories",
  "info": {
    "singularName": "merchant-mini-app-category",
    "pluralName": "merchant-mini-app-categories",
    "displayName": "Merchant MiniApp Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "component",
      "repeatable": false,
      "component": "merchant.translation",
      "required": true
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "is_home": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "mini_app_category_type": {
      "type": "enumeration",
      "enum": [
        "popular",
        "core",
        "service"
      ],
      "required": true
    },
    "color": {
      "type": "string",
      "required": true
    },
    "position": {
      "type": "biginteger",
      "required": true
    },
    "merchant_mini_apps": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::merchant-mini-app-mini-app.merchant-mini-app-mini-app",
      "mappedBy": "merchant_mini_app_category"
    },
    "tag": {
      "type": "enumeration",
      "enum": [
        "category"
      ],
      "required": true,
      "default": "category"
    }
  }
}

