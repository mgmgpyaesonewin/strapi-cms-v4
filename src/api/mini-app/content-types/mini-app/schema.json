{
  "kind": "collectionType",
  "collectionName": "mini_apps",
  "info": {
    "singularName": "mini-app",
    "pluralName": "mini-apps",
    "displayName": "Mini App",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "component",
      "repeatable": false,
      "component": "wp.default-label"
    },
    "deep_link": {
      "displayName": "deep_link",
      "type": "component",
      "repeatable": false,
      "component": "wp.deep-link"
    },
    "is_home": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "position": {
      "type": "integer",
      "required": true
    },
    "include_header": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "path": {
      "displayName": "path",
      "type": "component",
      "repeatable": true,
      "component": "wp.path"
    },
    "parameters": {
      "displayName": "parameters",
      "type": "component",
      "repeatable": true,
      "component": "wp.parameters"
    },
    "mini_app_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::mini-app-category.mini-app-category",
      "inversedBy": "mini_apps"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
