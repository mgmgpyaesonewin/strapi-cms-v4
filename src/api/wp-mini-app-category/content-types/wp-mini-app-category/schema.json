{
  "kind": "collectionType",
  "collectionName": "wp_mini_app_categories",
  "info": {
    "singularName": "wp-mini-app-category",
    "pluralName": "wp-mini-app-categories",
    "displayName": "WP MiniApp Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "component",
      "repeatable": false,
      "component": "wp.default-label",
      "required": true
    },
    "position": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "mini_apps": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::wp-mini-app.wp-mini-app",
      "mappedBy": "mini_app_category"
    },
    "tag": {
      "type": "enumeration",
      "enum": [
        "category"
      ],
      "default": "category",
      "required": true
    }
  }
}
