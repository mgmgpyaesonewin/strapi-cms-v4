    },
    "title": {
      "type": "component",
      "repeatable": false,
      "component": "wc.wc-translation",
      "required": true
    },
    "description": {
      "type": "component",
      "repeatable": false,
      "component": "wc.wc-translation",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "position": {
      "type": "biginteger",
      "required": true
    },
    "wc_tutorial_list": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::wc-tutorial-list.wc-tutorial-list",
      "inversedBy": "subcategories"
