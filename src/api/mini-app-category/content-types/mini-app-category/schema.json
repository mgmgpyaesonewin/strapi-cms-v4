{
  "kind": "collectionType",
  "collectionName": "mini_app_categories",
  "info": {
    "singularName": "mini-app-category",
    "pluralName": "mini-app-categories",
    "displayName": "Mini App Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "component",
      "repeatable": false,
      "component": "wp.default-label"
    },
    "home": {
      "type": "boolean",
      "required": true
    },
    "position": {
      "type": "integer",
      "required": true
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "mini_apps": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mini-app.mini-app",
      "mappedBy": "mini_app_category"
    }
  }
}
