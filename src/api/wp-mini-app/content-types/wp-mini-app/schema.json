{
  "kind": "collectionType",
  "collectionName": "wp_mini_apps",
  "info": {
    "singularName": "wp-mini-app",
    "pluralName": "wp-mini-apps",
    "displayName": "WP MiniApp MiniApp",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "component",
      "repeatable": false,
      "component": "wp.default-label",
      "required": true
    },
    "position": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "include_header": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "paths": {
      "displayName": "path",
      "type": "component",
      "repeatable": true,
      "component": "wp.path"
    },
    "parameters": {
      "displayName": "parameters",
      "type": "component",
      "repeatable": true,
      "component": "wp.parameters"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "mini_app_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::wp-mini-app-category.wp-mini-app-category",
      "inversedBy": "mini_apps"
    },
    "deep_link": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::deeplink.deeplink",
      "inversedBy": "wp_mini_apps"
    },
    "is_login": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "screen_orientation": {
      "type": "enumeration",
      "enum": [
        "portrait",
        "landscape",
        "neutral"
      ],
      "required": false
    },
    "color": {
      "type": "string",
      "required": true,
      "default": "#000000"
    },
    "tag": {
      "type": "enumeration",
      "enum": [
        "mini_app"
      ],
      "required": true,
      "default": "mini_app"
    },
    "wp_app_version_lists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::wp-app-version.wp-app-version"
    },
    "display": {
      "type": "enumeration",
      "enum": [
        "fullscreen",
        "native"
      ]
    },
    "is_service": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "wp_home_widget": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::wp-home-widget.wp-home-widget"
    },
    "mini_app_type": {
      "type": "enumeration",
      "enum": [
        "popular",
        "service"
      ]
    }
  }
}
