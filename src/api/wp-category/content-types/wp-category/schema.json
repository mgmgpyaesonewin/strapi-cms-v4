{
  "kind": "collectionType",
  "collectionName": "wp_categories",
  "info": {
    "singularName": "wp-category",
    "pluralName": "wp-categories",
    "displayName": "WP Promotion Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "category_title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "category_id": {
      "type": "integer",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "position": {
      "required": true,
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "min": 1
    },
    "photo_path_normal": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "photo_path_selected": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "wp_promotions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::wp-promotion.wp-promotion",
      "mappedBy": "wp_category"
    }
  }
}
