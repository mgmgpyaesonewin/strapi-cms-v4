{
  "kind": "collectionType",
  "collectionName": "wp_categories",
  "info": {
    "singularName": "wp-category",
    "pluralName": "wp-categories",
    "displayName": "WP Promotion Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {}
    },
    "position": {
      "required": true,
      "type": "integer",
      "pluginOptions": {},
      "min": 1
    },
    "photo_path_normal": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {}
    },
    "photo_path_selected": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {}
    },
    "wp_promotions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::wp-promotion.wp-promotion",
      "mappedBy": "wp_category"
    },
    "category_title": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {},
      "component": "wp.default-label",
      "required": true
    }
  }
}
