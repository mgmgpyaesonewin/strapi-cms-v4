{
  "kind": "collectionType",
  "collectionName": "wp_promotions",
  "info": {
    "singularName": "wp-promotion",
    "pluralName": "wp-promotions",
    "displayName": "WP Promotion Promotion",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "photo_path": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "hasDetails": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "action_link_android": {
      "type": "text"
    },
    "action_link_ios": {
      "type": "text",
      "required": false
    },
    "is_external": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "external_deeplink": {
      "type": "text"
    },
    "promotion_code": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "position": {
      "type": "integer",
      "min": 1,
      "required": true
    },
    "wp_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::wp-category.wp-category",
      "inversedBy": "wp_promotions"
    },
    "wp_promotion_type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::wp-promotion-type.wp-promotion-type",
      "inversedBy": "wp_promotions"
    },
    "promotion_details": {
      "type": "text",
      "required": false
    },
    "kyc_level_check": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "new_action_link_android": {
      "type": "text"
    },
    "title": {
      "type": "component",
      "repeatable": false,
      "component": "wp.default-label",
      "required": true
    },
    "paths": {
      "type": "component",
      "repeatable": true,
      "component": "wp.path"
    },
    "parameters": {
      "type": "component",
      "repeatable": true,
      "component": "wp.parameters"
    },
    "is_login": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "display": {
      "type": "enumeration",
      "enum": [
        "fullscreen",
        "native"
      ]
    },
    "include_header": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "wp_deeplink": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::deeplink.deeplink"
    },
    "wp_feature_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::wp-feature-id.wp-feature-id"
    },
    "wp_home_widget": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::wp-home-widget.wp-home-widget"
    }
  }
}
