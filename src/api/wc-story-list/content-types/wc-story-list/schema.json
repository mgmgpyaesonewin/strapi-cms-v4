{
  "kind": "collectionType",
  "collectionName": "wc_story_lists",
  "info": {
    "singularName": "wc-story-list",
    "pluralName": "wc-story-lists",
    "displayName": "WC Story List",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "story_name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "story"
      ],
      "default": "story",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "title": {
      "type": "component",
      "repeatable": false,
      "component": "wc.wc-translation",
      "required": true
    },
    "description": {
      "type": "component",
      "repeatable": false,
      "component": "wc.wc-translation",
      "required": true
    },
    "position": {
      "type": "biginteger",
      "required": true
    },
    "wc_story_contents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::wc-story-content.wc-story-content",
      "mappedBy": "wc_story_list"
    }
  }
}
